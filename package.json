  "scripts": {
    "gatsby:dev": "gatsby develop",
    "gatsby:build": "NODE_ENV=production GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "gatsby:serve": "gatsby serve",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "clean": "rm -rf _site .cache public",
    "__start": "npm-run-all clean --parallel dev:*",
    "__build": "npm-run-all clean --parallel prod:*",

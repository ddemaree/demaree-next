{
  "name": "demaree-leventy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "old__start": "eleventy --serve & postcss styles/global.css --o _tmp/style.css --watch",
    "old__build": "ELEVENTY_PRODUCTION=true eleventy & ELEVENTY_PRODUCTION=true postcss styles/global.css --o _site/style.css",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "clean": "rm -rf _site",
    "start": "npm-run-all clean --parallel dev:*",
    "dev:eleventy": "ELEVENTY_ENV=development eleventy --watch --quiet",
    "dev:modewebpack": "webpack --mode development --watch",
    "build": "npm-run-all clean --parallel prod:*",
    "prod:eleventy": "ELEVENTY_ENV=production ELEVENTY_PRODUCTION=true eleventy --output=./_site",
    "prod:webpack": "NODE_ENV=production webpack -p"
  },
  "author": "",

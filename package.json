    "alpine": "^0.2.1",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "browser-sync-webpack-plugin": "^2.2.2",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-minifier": "^4.0.0",
    "imagemin-webp-webpack-plugin": "^3.3.1",
    "markdown-it-attrs": "^3.0.2",
